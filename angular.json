{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "pms-web": {
      "root": "apps/pms-web/",
      "sourceRoot": "apps/pms-web/src",
      "projectType": "application",
      "prefix": "pms",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/pms-web",
            "index": "apps/pms-web/src/index.html",
            "main": "apps/pms-web/src/main.ts",
            "polyfills": "apps/pms-web/src/polyfills.ts",
            "tsConfig": "apps/pms-web/tsconfig.app.json",
            "assets": [
              "apps/pms-web/src/favicon.ico",
              "apps/pms-web/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "apps/pms-web/src/styles/antd/theme.less",
              "apps/pms-web/src/styles/bootstrap/bootstrap.scss",
              "apps/pms-web/src/styles/pms/pms.scss",
              "apps/pms-web/src/styles.scss"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/pms-web/src/environments/environment.ts",
                  "with": "apps/pms-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pms-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pms-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pms-web:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/pms-web/tsconfig.app.json",
              "apps/pms-web/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/pms-web/jest.config.js",
            "tsConfig": "apps/pms-web/tsconfig.spec.json",
            "setupFile": "apps/pms-web/src/test-setup.ts"
          }
        }
      }
    },
    "pms-web-e2e": {
      "root": "apps/pms-web-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/builders:cypress",
          "options": {
            "cypressConfig": "apps/pms-web-e2e/cypress.json",
            "tsConfig": "apps/pms-web-e2e/tsconfig.e2e.json",
            "devServerTarget": "pms-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "pms-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/pms-web-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "api-gub": {
      "root": "apps/api-gub/",
      "sourceRoot": "apps/api-gub/src",
      "projectType": "application",
      "prefix": "ah",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/api-gub",
            "index": "apps/api-gub/src/index.html",
            "main": "apps/api-gub/src/main.ts",
            "polyfills": "apps/api-gub/src/polyfills.ts",
            "tsConfig": "apps/api-gub/tsconfig.app.json",
            "assets": [
              "apps/api-gub/src/favicon.ico",
              "apps/api-gub/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "apps/pms-web/src/styles/antd/theme.less",
              "apps/pms-web/src/styles/bootstrap/bootstrap.scss",
              "apps/pms-web/src/styles/pms/pms.scss",
              "apps/api-hub/src/styles.scss"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/api-gub/src/environments/environment.ts",
                  "with": "apps/api-gub/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "api-gub:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "api-gub:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "api-gub:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api-gub/tsconfig.app.json",
              "apps/api-gub/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "apps/api-gub/jest.config.js",
            "tsConfig": "apps/api-gub/tsconfig.spec.json",
            "setupFile": "apps/api-gub/src/test-setup.ts"
          }
        }
      }
    },
    "api-gub-e2e": {
      "root": "apps/api-gub-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/builders:cypress",
          "options": {
            "cypressConfig": "apps/api-gub-e2e/cypress.json",
            "tsConfig": "apps/api-gub-e2e/tsconfig.e2e.json",
            "devServerTarget": "api-gub:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "api-gub:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/api-gub-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "pms-api": {
      "root": "libs/pms-api",
      "sourceRoot": "libs/pms-api/src",
      "projectType": "library",
      "prefix": "pms-client",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/pms-api/tsconfig.lib.json",
              "libs/pms-api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/builders:jest",
          "options": {
            "jestConfig": "libs/pms-api/jest.config.js",
            "tsConfig": "libs/pms-api/tsconfig.spec.json",
            "setupFile": "libs/pms-api/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics"
  },
  "schematics": {
    "@nrwl/schematics:application": {
      "style": "scss"
    },
    "@nrwl/schematics:library": {
      "style": "scss"
    }
  },
  "defaultProject": "pms-web"
}
